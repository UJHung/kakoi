// prisma/schema.prisma
datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model UserCard {
  id          String   @id @default(cuid())
  issuer      String
  productName String
  nickname    String?
  last4       String?
  createdAt   DateTime @default(now())
}
